{
  "name": "edno/codeception-gherkin-param",
  "description": "Codeception extension for supporting parameter notation in Gherkin features",
  "keywords": ["codeception", "extension", "gherkin"],
  "homepage": "https://github.com/edno/codeception-gherkin-param",
  "license": "Apache-2.0",
  "authors": [
      {
        "name": "Gregory Heitz",
        "email": "edno@edno.io"
      }
  ],
  "require": {
    "php": ">=7.0",
    "codeception/codeception": ">=2.2"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2",
    "codeception/module-asserts": "^1"
  },
  "autoload": {
    "psr-4": {
	  "Codeception\\Extension\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
