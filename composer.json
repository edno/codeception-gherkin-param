{
  "name": "edno/codeception-gherkin-param",
  "description": "Codeception module for supporting parameter notation in Gherkin features",
  "keywords": ["codeception", "extension", "gherkin", "bdd", "module", "test"],
  "homepage": "https://edno.github.io/codeception-gherkin-param",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Gregory Heitz",
      "email": "edno@edno.io"
    }
  ],
  "require": {
    "php": ">=7.2 <9.0",
    "codeception/codeception": "3 - 5",
    "composer-runtime-api": "^2.0"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2",
    "codeception/module-asserts": "^1",
    "infection/infection": "^0.24",
    "infection/codeception-adapter": "^0.3.4"
  },
  "autoload": {
    "psr-4": {
      "Codeception\\Extension\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
